{
  "manifest_version": 2,

  "name": "Stocker",
  "description": "Set when you want to buy/sell stocks",
  "version": "1.0",

  "browser_action": {
    "default_icon": "res/icon128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "http://*.query.yahooapis.com/*",
    "http://*/*",
    "https://*/*",
    "*://*.google.com/"
    
  ],
  "icons": { 
    "128": "res/icon128.png" 
  }
}
